<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Foreign_Country</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
ISPICKVAL(Status, &apos;Qualified&apos;), 
ISPICKVAL(Foreign_National_picklist__c, &quot;Yes&quot;), 
ISPICKVAL(Country__c, &quot;&quot;) 
)</errorConditionFormula>
    <errorDisplayField>Country__c</errorDisplayField>
    <errorMessage>Please Select a Country.</errorMessage>
</ValidationRule>
