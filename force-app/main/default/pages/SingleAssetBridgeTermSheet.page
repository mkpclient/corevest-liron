<apex:page standardController="Opportunity" extensions="TermSheetPDF_Controller" applyBodyTag="false" renderAs="pdf">
<!--<apex:variable id = "logo" var="logo" value="{!logo}"/> -->    
<apex:variable id="logo1" var="logo1" value="{!$Resource.corevestLogo}" />     
<apex:variable id="logo2" var="logo2" value="{!$Resource.corevestRiverbendLogo}" />
<!--<apex:variable id = "logoall" var="logoall" value= "{!IF(opportunity.stagename == 'testing',logo1,{!logo2})}" />   -->
   
   
<head>
    <style type="text/css">
      /*@page {
      @bottom-right {
      content: element(footer);
      }
      }

      div.header {
      padding: 10px;
      }

      div.footer {
      display: ;
      padding: 5px;
      position: running(footer);
      }*/

      .pagenumber:before {
        content: counter(page);
      }

      .pagecount:before {
        content: counter(pages);
      }

      .pagebreak {
        page-break-before: always;
      }

      @page {
        @top-center {
          content: element(header);
        }
        @bottom-center {
          content: element(footer);
        }
        margin: .9in;
        margin-top: 1.25in;
      }
    
     div.header {
        text-align: left;
            background: url("{!IF(Opportunity.Owner.Originating_Company__c == 'Riverbend', logo2 , logo1)}") no-repeat;
        width: 300px;
        height: 68px;
        margin top: 1in;
        padding: 10px;
        position: running(header);
      }
      
      div.footer {
        display: block;
        padding: 5px;
        position: running(footer);
        text-align: center;
        font-family: Times;
        font-size: 10pt;
      }

      div.spinitials {
        display: block;
        padding: 5px;
        position: running(footer);
        text-align: right;
        font-family: Times;
      }

      .sigsection {
        page-break-inside: avoid;
      }

      .pagebreak {
        page-break-before: always;
      }

      .signing-section {
        text-decoration: underline;
        color: white;
        border-bottom-style: solid;
        border-bottom-width: 1px;
        border-color: black;
      }

      .paragraph-title {
        text-align: center;
        font-family: Times;
        font-size: 11pt;
        font-weight: bold;
        text-decoration: underline;
      }

      .paragraph {
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        text-align: justify;
        text-indent: 40px;
      }

      .row-title {
        font-family: Times;
        font-size: 10pt;
        width: 30%;
        font-weight: bold;
        vertical-align: top;
        padding: 2px 6px;
        border-right: solid 1px #BEBEBE;
        border-bottom: solid 1px #BEBEBE;
        border-top: solid 1px #BEBEBE;
        border-left: solid 1px #BEBEBE;
      }

      .row-description {
        font-family: Times;
        font-size: 10pt;
        width: 70%;
        padding: 2px 6px;
        vertical-align: top;
        border-right: solid 1px #BEBEBE;
        border-bottom: solid 1px #BEBEBE;
        border-top: solid 1px #BEBEBE;
        border-left: solid 1px #BEBEBE;
      }
    </style>
  </head>
  <div class="footer">
    <table style="width: 100%">
      <tr>
        <td style="
            width: 30%;
            vertical-align: top;
            padding-top: 1px;
            font-family: Times;
            font-size: 7pt;
            text-align: left;
          ">
          Single Asset Bridge Loan
          <br /> Loan Number:
          <apex:outputText value=" {!Opportunity.Deal_Loan_Number__c}" />
          <br /> Date Generated:
          <apex:outputText value=" {0,date, MMMM d',' yyyy}">
            <apex:param value="{! DATE(todayYear, todayMonth, todayDay)}" /></apex:outputText>
          <br /> Version 1.5
        </td>
        <td style="
            width: 40%;
            vertical-align: top;
            padding-top: 1px;
            font-family: Times;
            font-size: 10pt;
            text-align: center;
          ">
          <div>
            Page
            <span class="pagenumber" /> of
            <span class="pagecount" />
          </div>
        </td>
        <td style="
            width: 30%;
            vertical-align: top;
            padding-top: 1px;
            font-family: Times;
            font-size: 10pt;
            text-align: right;
          ">
          Sponsor Initials ________
        </td>
      </tr>
    </table>
  </div>
  <div class="header" style="margin-top: 0.3in; margin-left: 0.05in">
    <!--Logo-->
  </div>

  <div class="content">
    <p style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
      ">
      <apex:outputText value=" {0,date, MMMM d',' yyyy}">
        <apex:param value="{! DATE(todayYear, todayMonth, todayDay)}" /></apex:outputText>
    </p>
    <p style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
      ">
      <!-- <apex:outputText value="{!Opportunity.Sponsor_Entity__c}" /> -->
      <apex:outputField value="{!Opportunity.Account.BillingStreet}" />
      <br />
      <apex:outputText value="{!Opportunity.Account.BillingCity}, " />
      <apex:outputText value="{!Opportunity.Account.Billing_State_Code__c} " rendered="{!NOT (ISBLANK(Opportunity.Account.BillingState))}"
      />
      <apex:outputText value="{!Opportunity.Account.BillingPostalCode}" />
      <br />

      <span style="{!IF(NOT(Opportunity.Account.BillingCountry == 'United States'), 'page-break-inside:avoid;', 'display:none;')}">
        <apex:outputText value="{!Opportunity.Account.BillingCountry}" rendered="{!NOT(Opportunity.Account.BillingCountry == 'United States')}"
        />
        <br />
      </span>
      <apex:outputText value="{!'Attention: ' & Opportunity.Primary_Contact__c}" /> ("
      <strong>Sponsor</strong>")
      <!--<apex:outputText value="{!'Attention: ' & Opportunity.Sponsor_Entity__c}" />  -->
    </p>
    <p style="{!IF(includeCOBroker = true, 'page-break-inside:avoid;', 'display:none;')}">
      <span style="
          font-family: Times;
          font-size: 10pt;
          line-height: 1.3;
          page-break-inside: avoid;
        ">
        C/O:
        <apex:outputText value=" {!Opportunity.Referral_Source_Contact__r.Name} " /> -
        <apex:outputText value=" {!Opportunity.Referral_Source__r.Name}" />
      </span>
    </p>
    <p style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
      ">
      Re:&nbsp;&nbsp;
      <apex:outputText value=" ${0,number}">
        <apex:param value="{!Opportunity.Loan_Size__c}" /></apex:outputText>
      Bridge Loan ("
      <strong>Loan</strong>") for
      <apex:outputText value=" {! sabProperty.Name }, {! sabProperty.City__c }, {! sabProperty.State__c } {! sabProperty.ZipCode__c } ">
      </apex:outputText>
      ("
      <strong>Property</strong>")
    </p>
    <p style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
      ">
      Dear &nbsp;
      <!-- <apex:outputText value="{!Opportunity.Sponsor_Entity__c}" />: -->
      <apex:outputText value="{!Opportunity.Contact__r.FirstName}" />:
    </p>
    <p style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
        text-indent: 30px;
        text-align: justify;
      ">
      CoreVest American Finance Lender LLC or an affiliated entity
      <strong>("Lender")</strong> is pleased to present this summary proposal to provide a secured bridge loan for the purchase and/or
      financing of the aforementioned property. As further set forth below, this proposal is for discussion purposes only
      and is neither an expressed nor implied commitment to provide any financing or to provide or purchase any loans or
      securities in connection with the transactions contemplated hereby.
    </p>
    <!--Loan Details Table-->
    <table style="
        table-layout: fixed;
        width: 95vw;
        border-collapse: collapse;
        border: 1px solid #bebebe;
      " border="1" cellspacing="0">
      <colgroup>
        <col class="short" />
        <col class="long" />
      </colgroup>

      <tr style="page-break-inside: avoid">
        <td class="row-title">
          <strong> Maximum Loan Amount: </strong>
        </td>
        <td class="row-description">
          $
          <apex:outputText value="{0, number, 000,000}">
            <apex:param value="{! Opportunity.Loan_Size__c }" /></apex:outputText>
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          <strong> Loan Type: </strong>
        </td>
        <td class="row-description">
          Senior Secured First Mortgage Loan
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          <strong> Borrower Entity: </strong>
        </td>
        <td class="row-description">
          <apex:outputText value="{!if(isLegalEntityOnly, 'Loan will be made to a US legal entity', 'Loan will be made to an individual or a US legal entity')}" />
          Loan will be made to an individual or a US legal entity
          <br />("
          <strong>Borrower</strong>")
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          <strong> Guarantor: </strong>
        </td>
        <td class="row-description">
          The undersigned Sponsor and/or such individuals or entities acceptable to Lender following due diligence
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          <strong> Loan Purpose: </strong>
        </td>
        <td class="row-description">
          Purchase or refinance of an eligible property recently acquired by Borrower for a <apex:outputText value=" {!Opportunity.LOC_Loan_Type__c}" /> 
          with <apex:outputText value=" {!Opportunity.Product_Sub_Type__c}" /> for the purpose of <apex:outputText value=" {!Opportunity.Project_Strategy__c}" />
          <!-- <apex:outputPanel rendered="{!Opportunity.Product_Sub_Type__c != 'Renovation'}">
            Purchase or refinance of an eligible property recently acquired by Borrower for a 
          </apex:outputPanel>
          <apex:outputPanel rendered="{!Opportunity.Product_Sub_Type__c == 'Renovation'}">
            Purchase or refinance of an eligible property recently acquired by Borrower and, if applicable, reimbursement of subsequent
            Lender approved renovation costs related to the Property
          </apex:outputPanel> -->
        </td>
      </tr>
      <!-- <tr style="page-break-inside:avoid;">
<td style="font-family: Times; font-size: 10pt; width:30%;font-weight:bold;vertical-align:top;padding:2px 6px; border-collapse: collapse; border: 1px solid #BEBEBE;">
<strong>
Equity Pledge:
</strong>
</td>
<td style="font-family: Times; font-size: 10pt; width:70%;padding:2px 6px;vertical-align:top;border-collapse: collapse; border: 1px solid #BEBEBE;">
<apex:outputText rendered="{! If(Opportunity.Loan_Size__c >= 10000000, true, false) }" value="Yes" />
