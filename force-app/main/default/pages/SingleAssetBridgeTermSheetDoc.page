<apex:page standardController="Opportunity" extensions="TermSheetPDF_Controller" contentType="application/x-unknown"
    cache="true">
    <!-- application/msword#{!Opportunity.Name}_TermSheetv_{!intStr}.doc -->
    <head>
        <style type="text/css">
            p.MsoHeader,
            li.MsoHeader,
            div.MsoHeader {
                margin: 0in;
                margin-top: .0001pt;
                mso-pagination: widow-orphan;
                tab-stops: center 3.0in right 6.0in;
            }

            p.MsoFooter,
            li.MsoFooter,
            div.MsoFooter {
                margin: 0in;
                margin-bottom: .0001pt;
                mso-pagination: widow-orphan;
                tab-stops: center 3.0in right 6.0in;
            }

            @page Section1 {
                size: 8.5in 11.0in;
                /* margin:0.5in 0.5in 0.5in 0.5in; */
                margin: 0.5in 1in 1in 0.75in;
                mso-header-margin: 0.5in;
                mso-header: h1;
                mso-footer: f1;
                mso-footer-margin: 0.5in;
                mso-paper-source: 0;
            }

            div.Section1 {
                page: Section1;
            }

            /*Below is the style to prevent the repetition of header and Footer.. Cheers!!! */

            table#hrdftrtbl {
                margin: 0in 0in 0in 9in;
            }

            @page {
                @bottom-right {
                    content: element(footer);
                }
            }

            .pagenumber:before {
                content: counter(page);
            }

            .pagecount:before {
                content: counter(pages);
            }

            .pagebreak {
                page-break-before: always;
            }

            div.header {
                /* text-align: left; */
                /* background: url("{! $Resource.corevestLogo }") no-repeat; */
                /* width: 320px;
            height: 68px; */
                /* margin top: 2in; */
                padding: 10px;
                position: running(header);
                display: block;
            }

            div.footer {
                display: block;
                padding: 5px;
                position: running(footer);
                text-align: center;
                font-family: Times;
            }

            div.spinitials {
                display: block;
                padding: 5px;
                position: running(footer);
                text-align: right;
                font-family: Times;
            }

            .sigsection {
                page-break-inside: avoid;
            }
        </style>
    </head>

    <!--<div class="Section1">-->
    <div class="header">
        <div>
            <apex:image url="https://cvest.my.salesforce.com/servlet/servlet.ImageServer?id=0150a000001fqte&oid=00Dj0000000L4sC" />
        </div>
    </div>
