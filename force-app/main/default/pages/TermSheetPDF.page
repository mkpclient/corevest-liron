<apex:page standardController="Opportunity"
  extensions="TermSheetPDF_Controller"
  applyBodyTag="false"
  renderAs="pdf"
>
  <!-- <apex:outputText rendered="{!}">
</apex:outputText> -->

  <head>
    <style type="text/css">
      @page {
        @top-center {
          content: element(header);
        }
        @bottom-center {
          content: element(footer);
        }
        margin: 0.9in;
        margin-top: 1.25in;
      }

      .header {
        text-align: left;
        background: url("{! $Resource.corevestLogo }") no-repeat;
        width: 320px;
        height: 68px;
        /* margin top: 2in; */
        padding: 10px;
        position: running(header);
      }

      div.footer {
        display: block;
        padding: 5px;
        position: running(footer);
        text-align: center;
        font-family: Times;
      }

      .pagenumber:before {
        content: counter(page);
      }

      .pagecount:before {
        content: counter(pages);
      }

      .pagebreak {
        page-break-before: always;
      }

      .signing-section {
        text-decoration: underline;
        color: white;
        border-bottom-style: solid;
        border-bottom-width: 1px;
        border-color: black;
      }

      .definition-title {
        vertical-align: top;
        font-family: Times;
        font-size: 10pt;
        font-weight: bold;
        line-height: 1.3;
        padding: 5px 6px;
        width: 25%;
      }

      .definition {
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        text-align: justify;
        vertical-align: top;
        padding: 5px 6px;
      }

      .sub-definition-title {
        vertical-align: top;
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        padding: 5px 6px;
        width:25%;
        text-decoration:underline;
      }

      .sub-definition {
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        text-align: justify;
        vertical-align: top;
        padding: 5px 6px;
      }

      .row-title {
        font-family: Times;
        font-size: 10pt;
        padding: 2px 6px;
        vertical-align: top;
        width: 25%;
        font-weight: bold;
        border-right: solid 1px #bebebe;
        border-bottom: solid 1px #bebebe;
        border-top: solid 1px #bebebe;
        border-left: solid 1px #bebebe;
      }

      .row-description {
        font-family: Times;
        font-size: 10pt;
        padding: 2px 6px;
        vertical-align: top;
        width: 75%;
        border-right: solid 1px #bebebe;
        border-bottom: solid 1px #bebebe;
        border-top: solid 1px #bebebe;
        border-left: solid 1px #bebebe;
      }
    </style>
  </head>

  <div class="footer">
    <!--<div>Page
<span class="pagenumber" /> of
<span class="pagecount" />
</div>-->
    <table style="width: 100%">
      <tr>
        <td
          style="
            width: 30%;
            vertical-align: top;
            padding-top: 1px;
            font-family: Times;
            font-size: 7pt;
            text-align: left;
          "
        >
          Term Loan
          <br /> Loan Number:
          <apex:outputText value=" {!Opportunity.Deal_Loan_Number__c}" />
          <br /> Date Generated:
          <apex:outputText value=" {0,date, MMMM d',' yyyy}">
            <apex:param value="{! DATE(todayYear, todayMonth, todayDay)}"
          /></apex:outputText>
          <br /> Version 1.0
        </td>
        <td
          style="
            width: 40%;
            vertical-align: top;
            padding-top: 1px;
            font-family: Times;
            font-size: 10pt;
            text-align: center;
          "
        >
          <div>
            Page <span class="pagenumber" /> of
            <span class="pagecount" />
          </div>
        </td>
        <td
          style="
            width: 30%;
            vertical-align: top;
            padding-top: 1px;
            font-family: Times;
            font-size: 10pt;
            text-align: right;
          "
        >
          <!--Sponsor Initials ________-->
        </td>
      </tr>
    </table>
  </div>

  <div class="header" style="margin-top: 0.3in; margin-left: 0.05in">
    <!--<div>
<apex:image url="{!$Resource.corevestLogo}" width="200" />
</div>-->
  </div>

  <div class="content">
    <p
      style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
      "
    >
      <!-- <apex:outputText value="{!TEXT(MONTH(Opportunity.Term_Sheet_Issued__c)) & '/' & TEXT(DAY(Opportunity.Term_Sheet_Issued__c)) & '/' & TEXT(YEAR(Opportunity.Term_Sheet_Issued__c))}" /> -->
      <apex:outputText value=" {0,date, MMMM d',' yyyy}">
        <apex:param value="{! DATE(todayYear, todayMonth, todayDay)}"
      /></apex:outputText>
    </p>
    <p
      style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
      "
    >
      <!-- {!Opportunity.Name}<br /> -->
      <!--<apex:outputText value="{!Opportunity.Sponsor_Entity__c}" rendered="{! NOT( ISBLANK(Opportunity.Sponsor_Entity__c) ) }" />
<apex:outputText value="{!Opportunity.Primary_Contact__c}" rendered="{! ISBLANK(Opportunity.Sponsor_Entity__c) }" /><br />-->
      <!-- <apex:outputText escape="false" value="{!Opportunity.Account.BillingStreet}" /> -->
      <apex:outputField value="{!Opportunity.Account.BillingStreet}" />
      <br />
      <apex:outputText value="{!Opportunity.Account.BillingCity}, " />
      <apex:outputText value="{!Opportunity.Account.Billing_State_Code__c} "
        rendered="{!NOT (ISBLANK(Opportunity.Account.BillingState))}"
      />
      <apex:outputText value="{!Opportunity.Account.BillingPostalCode}" />
      <br />
      <span
        style="{!IF(NOT(Opportunity.Account.BillingCountry == 'United States'), 'page-break-inside:avoid;', 'display:none;')}"
      >
        <apex:outputText value="{!Opportunity.Account.BillingCountry}"
          rendered="{!NOT(Opportunity.Account.BillingCountry == 'United States')}"
        />
        <br />
      </span>
      <!--<apex:outputText value="{!'Attention: ' & Opportunity.Primary_Contact__c}" rendered="{! NOT( ISBLANK(Opportunity.Sponsor_Entity__c) ) }" />-->
      Attention: &nbsp;
      <apex:outputText value="{!Opportunity.Primary_Contact__c}" />
    </p>
    <p
      style="{!IF(includeCOBroker = true, 'page-break-inside:avoid;', 'display:none;')}"
    >
      <span
        style="
          font-family: Times;
          font-size: 10pt;
          line-height: 1.3;
          page-break-inside: avoid;
        "
      >
        C/O:
        <apex:outputText value=" {!Opportunity.Referral_Source_Contact__r.Name} "
        />
        -
        <apex:outputText value=" {!Opportunity.Referral_Source__r.Name}" />
      </span>
    </p>
    <p
      style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
      "
    >
      <apex:outputText value="${0, number}">
        <apex:param value="{! Opportunity.Loan_Size__c }" />
      </apex:outputText>
      Portfolio Financing Proposal
    </p>
    <p
      style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
      "
    >
      Dear&nbsp;
      <apex:outputText value="{!Opportunity.Contact__r.FirstName}" />:
    </p>
    <p
      style="
        font-family: Times;
        font-size: 10pt;
        line-height: 1.3;
        page-break-inside: avoid;
        text-indent: 45px;
        text-align: justify;
      "
    >
      CoreVest American Finance Lender LLC (
      <strong>"Lender"</strong>) is pleased to present this summary of loan
      terms (this <strong>"Summary of Terms"</strong>) to provide a blanket
      first mortgage loan (the <strong>"Loan"</strong>) secured by your
      portfolio of
      <apex:outputText value=" {!totalPropertiesText}" />
      <apex:outputText value=" ({0,number,###})">
        <apex:param value="{!Opportunity.Total_Properties__c}"
      /></apex:outputText>
      <span style="{!if(totalPropertiesText == 'one', '', 'display:none' )}"
        >property,</span
      >
      <span style="{!if(totalPropertiesText != 'one', '', 'display:none' )}"
        >properties,</span
      >
      specifically set forth in Schedule 4 attached hereto and incorporated
      herein, which
      <span style="{!if(totalPropertiesText == 'one', '', 'display:none' )}"
        >is</span
      >
      <span style="{!if(totalPropertiesText != 'one', '', 'display:none' )}"
        >are</span
      >
      located in&nbsp;
      <apex:outputText value="{!Opportunity.Distinct_States_Full__c}" /> (each a
      <strong>"Property"</strong> and, collectively,
      <strong>"Properties"</strong> or the <strong>"Portfolio"</strong>). As set
      forth below, this Summary of Terms is for discussion purposes only and is
      neither an express nor implied commitment to provide any financing or to
      provide any loans in connection with the transactions contemplated hereby.
    </p>

    <table style="table-layout: fixed; width: 95vw" cellspacing="0">
      <colgroup>
        <col class="short" />
        <col class="long" />
      </colgroup>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Loan Amount:
        </td>
        <td class="row-description">
          <apex:outputText value="{0,number,###,###,###}">
            $
            <apex:param value="{!Opportunity.Loan_Size__c}" />
          </apex:outputText>
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">Loan Purpose:</td>
        <td class="row-description">
          <apex:outputText rendered="{!AND(NOT(ISBLANK(Opportunity.Refinance_Property_Count__c)), Opportunity.Refinance_Property_Count__c > 0)}"
            value="{!'Refinance on ' & TEXT(Opportunity.Refinance_Property_Count__c)}"
          />
          <apex:outputText rendered="{!AND(NOT(ISBLANK(Opportunity.Refinance_Property_Count__c)), Opportunity.Refinance_Property_Count__c == 1)}"
            value=" property"
          />
          <apex:outputText rendered="{!AND(NOT(ISBLANK(Opportunity.Refinance_Property_Count__c)), Opportunity.Refinance_Property_Count__c > 1)}"
            value=" properties"
          />

          <apex:outputText rendered="{!AND(NOT(ISBLANK(Opportunity.Refinance_Property_Count__c)), Opportunity.Refinance_Property_Count__c > 0, NOT(ISBLANK(Opportunity.Acquisition_Property_Count__c)), Opportunity.Acquisition_Property_Count__c > 0)}"
            value="{!' and '}"
          />

          <apex:outputText rendered="{!Opportunity.Acquisition_Property_Count__c != null && Opportunity.Acquisition_Property_Count__c > 0}"
            value="{!'Acquisition of ' & TEXT(Opportunity.Acquisition_Property_Count__c)}"
          />
          <apex:outputText rendered="{!AND(NOT(ISBLANK(Opportunity.Acquisition_Property_Count__c)), Opportunity.Acquisition_Property_Count__c == 1)}"
            value=" property"
          />
          <apex:outputText rendered="{!AND(NOT(ISBLANK(Opportunity.Acquisition_Property_Count__c)), Opportunity.Acquisition_Property_Count__c > 1)}"
            value=" properties"
          />

          <!-- <apex:outputText rendered="{!Opportunity.Aggregate_Payoffs__c != null && Opportunity.Aggregate_Payoffs__c > 0}" value="{!'Note: CAF Bridge Payoff on ' & TEXT(Opportunity.Aggregate_Payoffs__c) & ' properties. '}" /> -->
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">Maximum LTV:</td>
        <td class="row-description">
          Up to&nbsp;
          <apex:outputText value="{0,number,###.##}">
            <apex:param value="{!Opportunity.LTV__c}" /></apex:outputText>%
          <apex:outputText rendered="{!Opportunity.Acquisition_Count__c  != null && Opportunity.Acquisition_Count__c > 0}"
          >
            LTV to be based upon lesser of (a) as-is third-party value or (b)
            acquisition price
          </apex:outputText>
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Required/Threshold DSCR:
        </td>
        <td class="row-description">
          <!--<span style="{!IF(Opportunity.Loan_Size__c < 1000000, 'display:inline;', 'display:none;')}">
1.25x
</span>
<span style="{!IF(Opportunity.Loan_Size__c >= 1000000, 'display:inline;', 'display:none;')}">
1.20x
</span>-->
          <apex:outputText value="{!Opportunity.Min_DSCR__c}" />
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Interest Rate:
        </td>
        <td class="row-description">
          The interest rate for the Loan shall be fixed at a rate equal to the
          sum of (a) the
          <span
          style="display:{!IF(Opportunity.Term_Loan_Type__c != '30 Year','inline','none')}"
          >
            <apex:outputText value="{!Opportunity.Term_Loan_Type__c}" />
          </span>
          <span
          style="display:{!IF(Opportunity.Term_Loan_Type__c == '30 Year','inline','none')}"
          >
            5 Year&nbsp;
          </span>
          Swap Rate as of the Closing (see defined term below) Date, subject to a floor
          of&nbsp;
          <apex:outputText value="{0,number,###.00}">
            <apex:param value="{!Opportunity.Floor__c}" /></apex:outputText>% and (b)&nbsp;
          <apex:outputText value="{!Opportunity.Spread_BPS__c}" />% per annum.
          Today’s indicative rate is&nbsp;
          <apex:outputText value="{!Opportunity.Indicative_Rate_Calc__c}" />%.
          Pricing of base rate and margin is indicative and subject to
          re-pricing at Lender’s discretion based on factors that may include,
          but are not limited to, prevailing market conditions and
          underwriting/diligence review. Actual interest rate will be determined
          based on rate lock.
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Term:
        </td>
        <td class="row-description">
          <apex:outputText rendered="{!Opportunity.Term_Loan_Type__c == '5 Year'}"
            value="{!'Five (5) years'}"
          />
          <apex:outputText rendered="{!Opportunity.Term_Loan_Type__c == '7 Year'}"
            value="{!'Seven (7) years'}"
          />
          <apex:outputText rendered="{!Opportunity.Term_Loan_Type__c == '10 Year'}"
            value="{!'Ten (10) years'}"
          />
          <apex:outputText rendered="{!Opportunity.Term_Loan_Type__c == '30 Year'}"
            value="{!'Thirty (30) years'}"
          />
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Amortization:
        </td>
        <td class="row-description">
          <apex:outputText rendered="{!Opportunity.Amortization_Term__c == '360 months'}"
            value="{!'Thirty (30) years'}"
          />
          <apex:outputText rendered="{!Opportunity.Amortization_Term__c == '300 months'}"
            value="{!'Twenty-Five (25) years'}"
          />
          <apex:outputText rendered="{!Opportunity.Amortization_Term__c == '240 months'}"
            value="{!'Twenty (20) years'}"
          />
          <apex:outputText rendered="{!Opportunity.Amortization_Term__c == '180 months'}"
            value="{!'Fifteen (15) years'}"
          />
          <apex:outputText rendered="{!Opportunity.Amortization_Term__c == '120 months'}"
            value="{!'Ten (10) years'}"
          />
          <apex:outputText rendered="{!Opportunity.IO_Term__c != null && Opportunity.IO_Term__c != '0 months' && Opportunity.Amortization_Term__c !=null && Opportunity.Amortization_Term__c == '0 months'}"
            value="{!'None, interest only'}"
          />
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Origination Fee:
        </td>
        <td class="row-description">
          <div
            style="display:{!IF(Opportunity.Loan_Size__c >= 1000000,'block','none')}"
          >
            <div style="display:{!IF(includeBrokerFee = false,'block','none')}">
              <apex:outputText value="{0, number, ##.00}">
                <apex:param value="{!Opportunity.CAF_Upfront_Fee__c}" /></apex:outputText>% of the loan amount, due at closing
            </div>
            <div style="display:{!IF(includeBrokerFee = true,'inline','none')}">
              <apex:outputText value="{0, number, ##.00}">
                <apex:param value="{!Opportunity.CAF_Upfront_Fee__c}" /></apex:outputText>% of the loan amount paid to CoreVest and
              <apex:outputText value="{0, number, 0.00}%">
                <apex:param value="{! Opportunity.Broker_Fees__c}"
              /></apex:outputText>
              paid to the broker
            </div>
          </div>
          <div
            style="display:{!IF(Opportunity.Loan_Size__c < 1000000,'block','none')}"
          >
            <div style="display:{!IF(includeBrokerFee = false,'block','none')}">
              The greater of $7,500 or
              <apex:outputText value="{0, number, ##.00}">
                <apex:param value="{!Opportunity.CAF_Upfront_Fee__c}" /></apex:outputText>% of the loan amount, due at closing
            </div>
            <div style="display:{!IF(includeBrokerFee = true,'block','none')}">
              The greater of $7,500 or
              <apex:outputText value="{0, number, ##.00}">
                <apex:param value="{!Opportunity.CAF_Upfront_Fee__c}" /></apex:outputText>% of the loan amount paid to CoreVest and
              <apex:outputText value="{0, number, 0.00}%">
                <apex:param value="{! Opportunity.Broker_Fees__c}"
              /></apex:outputText>
              paid to the broker
            </div>
          </div>
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Closing:
        </td>
        <td class="row-description">
          Approximately sixty (60) days from the execution of this Summary of
          Terms (provided timely delivery of Due Diligence), on or about
          <apex:outputText value=" {0,date,MMMM d',' yyyy}">
            <apex:param value="{! DATE(todayYear, todayMonth, todayDay) + 60}" /></apex:outputText>. Any loan closing more than sixty (60) days after execution of
          Summary of Terms may be subject to additional underwriting fees.
        </td>
      </tr>

      <!-- <div class="pagebreak" style="page-break-before: always;"></div> -->

      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Recourse:
        </td>
        <td class="row-description">
          <span
            style="display:{!IF(Opportunity.Recourse__c == 'Recourse', 'inline','none')}"
          >
            Full recourse to Borrower, Pledgor, and Sponsor
          </span>
          <span
            style="display:{!IF(Opportunity.Recourse__c != 'Recourse', 'inline','none')}"
          >
            Non-recourse, except for environmental indemnification and Lender’s
            standard carve outs
          </span>
        </td>
      </tr>
      <tr
        style="{!IF(Opportunity.Cash_Management__c == 'None', 'page-break-inside:avoid;', 'display:none;')}"
      >
        <td class="row-title">
          Cash Management:
        </td>
        <td class="row-description">
          <apex:outputText value="{!Opportunity.Cash_Management__c}" />
        </td>
      </tr>
      <tr
        style="{!IF(Opportunity.Cash_Management__c != 'None', 'page-break-inside:avoid;', 'display:none;')}"
      >
        <td class="row-title">
          Cash Management:
        </td>
        <td class="row-description">
          <apex:outputText value="{!Opportunity.Cash_Management__c}" />, as
          described in Schedule "2"
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">
          Reserves:
        </td>
        <td class="row-description">
          Reserves required for taxes, insurance, capital expenditures
          <span
            style="display:{!IF(Opportunity.Recourse__c != 'Recourse' && Opportunity.Cash_Management__c != 'None', 'inline','none')}"
            >, advance rent and cash collateral</span
          >
          <span style="float: right; font-weight: bold"
            >Initials: <span class="signing-section">_____\i1\____</span></span
          >
        </td>
      </tr>
      <tr
        style="{!IF((Opportunity.Cash_Management__c == 'None' || Opportunity.Cash_Management__c == 'Springing') && Opportunity.Recourse__c == 'Non-Recourse', 'page-break-inside:avoid;', 'display:none;')}"
      >
        <td class="row-title">
          Holdback Reserve Amount:
        </td>
        <td class="row-description">
          <!-- <span style="{!IF(Opportunity.Total_Units__c >= 20, 'page-break-inside:avoid;', 'display:none;')}">Three (3) months of principal, interest, taxes, insurance and capital expenditures</span>
                    <span style="{!IF(Opportunity.Total_Units__c < 20, 'page-break-inside:avoid;', 'display:none;')}">Six (6) months of principal, interest, taxes, insurance and capital expenditures</span> -->
          <span
            style="{!IF(Opportunity.Total_Units__c >= 20 && (Opportunity.IO_Term__c == '0 months' || Opportunity.IO_Term__c == null), 'page-break-inside:avoid;', 'display:none;')}"
            >One (1) month of principal and interest</span
          >
          <span
            style="{!IF(Opportunity.Total_Units__c < 20 && (Opportunity.IO_Term__c == '0 months' || Opportunity.IO_Term__c == null), 'page-break-inside:avoid;', 'display:none;')}"
            >One (1) month of principal and interest</span
          >
          <span
            style="{!IF(Opportunity.Total_Units__c >= 20 && Opportunity.IO_Term__c != '0 months', 'page-break-inside:avoid;', 'display:none;')}"
            >One (1) month of interest</span
          >
          <span
            style="{!IF(Opportunity.Total_Units__c < 20 && Opportunity.IO_Term__c != '0 months', 'page-break-inside:avoid;', 'display:none;')}"
            >One (1) month of interest</span
          >
          <span style="float: right; font-weight: bold"
            >Initials: <span class="signing-section">_____\i1\____</span></span
          >
        </td>
      </tr>
      <tr style="page-break-inside: avoid">
        <td class="row-title">Par Prepayment Date:</td>
        <td class="row-description">
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '34 Months', 'inline','none')}"
          >
            34th Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '36 Months', 'inline','none')}"
          >
            36th Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '42 Months', 'inline','none')}"
          >
            42nd Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '48 Months', 'inline','none')}"
          >
            48th Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '52 Months', 'inline','none')}"
          >
            52nd Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '53 Months', 'inline','none')}"
          >
            53th Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '54 Months', 'inline','none')}"
          >
            54th Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '55 Months', 'inline','none')}"
          >
            55th Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '56 Months', 'inline','none')}"
          >
            56th Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '57 Months', 'inline','none')}"
          >
            57th Payment Date
          </span>
          <span
            style="display:{!IF(Opportunity.YM_Prepayment_Penalty__c == '58 Months', 'inline','none')}"
          >
            58th Payment Date
