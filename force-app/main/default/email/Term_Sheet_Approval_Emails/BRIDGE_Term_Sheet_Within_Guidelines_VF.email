    </td>
    </tr>
    
    <tr style="{!IF(ISBLANK(relatedTo.Broker_Fees__c), 'display:none;', '')}">
    <td style="border:1px solid black">
        Broker Fee
    </td>
    <td style="border:1px solid black;text-align:right;">
        <apex:outputText value="{0, number, #.##}%" rendered="{!NOT( ISBLANK(relatedTo.Broker_Fees__c) )}">
            <apex:param value="{!relatedTo.Broker_Fees__c}" />
        </apex:outputText>
        <apex:outputText value="00.00%" rendered="{!ISBLANK(relatedTo.Broker_Fees__c)}" />
    </td>
    </tr>
    
    <tr style="{!IF(ISBLANK(relatedTo.Broker_Fees__c), 'display:none;', '')}">
